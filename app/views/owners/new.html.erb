<%= form_for(@owner) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :country %>
  <%= f.text_field :country %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.submit "Sign Up as Owner" %>
<% end %>
